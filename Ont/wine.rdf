<?xml version="1.0"?>
<!DOCTYPE rdf:RDF [
     <!ENTITY vin  "http://www.w3.org/TR/2003/PR-owl-guide-20031209/Wyn#" >
     <!ENTITY food "http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#" >
     <!ENTITY owl  "http://www.w3.org/2002/07/owl#" >
     <!ENTITY xsd  "http://www.w3.org/2001/XMLSchema#" >
   ]>

<rdf:RDF
  xmlns     = "http://www.w3.org/TR/2003/PR-owl-guide-20031209/Wyn#"
  xmlns:vin = "http://www.w3.org/TR/2003/PR-owl-guide-20031209/Wyn#"
  xml:base  = "http://www.w3.org/TR/2003/PR-owl-guide-20031209/Wyn#"
  xmlns:food= "http://www.w3.org/TR/2003/PR-owl-guide-20031209/food#"
  xmlns:owl = "http://www.w3.org/2002/07/owl#"
  xmlns:rdf = "http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs= "http://www.w3.org/2000/01/rdf-schema#"
  xmlns:xsd = "http://www.w3.org/2001/XMLSchema#">

  <owl:Ontology rdf:about="">
    <rdfs:comment>An example OWL ontology</rdfs:comment>
    <owl:priorVersion>
      <owl:Ontology rdf:about="http://www.w3.org/TR/2003/CR-owl-guide-20030818/Wyn"/>
    </owl:priorVersion>
    <owl:imports rdf:resource="http://www.w3.org/TR/2003/PR-owl-guide-20031209/food"/>
    <rdfs:comment>Derived from the DAML Wyn ontology at 
      http://ontolingua.stanford.edu/doc/chimaera/ontologies/Wyns.daml
      Substantially changed, in particular the Streek based relations.
    </rdfs:comment>
    <rdfs:label>Wyn Ontology</rdfs:label>
  </owl:Ontology>
  
  <owl:Class rdf:ID="Wyn">
    <rdfs:subClassOf rdf:resource="&food;Drinkbare-Vloeistof" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-maker" />
	<owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-maker" />
	<owl:allValuesFrom rdf:resource="#wynmakery" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
 	<owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-suiker" />
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-geur" />
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-Body" />
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-kleur" />
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#gelee-in"/> 
        <owl:someValuesFrom rdf:resource="&vin;Streek"/>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Wyn</rdfs:label>
    <rdfs:label xml:lang="fr">vin</rdfs:label>
  </owl:Class>
  
  <owl:Class rdf:ID="Vintage">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-vintage-jaar"/>  
        <owl:cardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>

  <owl:Class rdf:ID="WynDruiwe">
    <rdfs:subClassOf rdf:resource="&food;Druiwe" />
  </owl:Class>
  
  <owl:Class rdf:ID="Witwyn">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Wyn" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-kleur" />
        <owl:hasValue rdf:resource="#Wit" />
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WitTafelwyn">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Tafelwyn" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-kleur" />
        <owl:hasValue rdf:resource="#Wit" />
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WitNieSoetWyn">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Witwyn" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-suiker" />
        <owl:allValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <owl:Thing rdf:about="#Droe" />
              <owl:Thing rdf:about="#AfDroe" />
            </owl:oneOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WitLoire">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Loire" />
      <owl:Class rdf:about="#Witwyn" />
    </owl:intersectionOf>
  </owl:Class>

  <owl:Class rdf:about="#WitLoire">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:allValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <owl:Thing rdf:about="#CheninBlancDruiwe" />
              <owl:Thing rdf:about="#PinotBlancDruiwe" />
              <owl:Thing rdf:about="#SauvignonBlancDruiwe" />
            </owl:oneOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WitBurgundy">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Burgundy" />
      <owl:Class rdf:about="#Witwyn" />
    </owl:intersectionOf>
  </owl:Class>

  <owl:Class rdf:about="#WitBurgundy">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:hasValue rdf:resource="#ChardonnayDruiwe" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WitBordeaux">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Bordeaux" />
      <owl:Class rdf:about="#Witwyn" />
    </owl:intersectionOf>
  </owl:Class>

  <owl:Class rdf:about="#WitBordeaux">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:allValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <owl:Thing rdf:about="#SemillonDruiwe" />
              <owl:Thing rdf:about="#SauvignonBlancDruiwe" />
            </owl:oneOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:ID="Streek" />

  <owl:ObjectProperty rdf:ID="gelee-in">
    <rdf:type rdf:resource="&owl;TransitiveProperty" />
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing" />
    <rdfs:range rdf:resource="#Streek" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="is-aangrensende-streek-van">
    <rdf:type rdf:resource="&owl;SymmetricProperty" />
    <rdfs:domain rdf:resource="#Streek" />
    <rdfs:range rdf:resource="#Streek" />
  </owl:ObjectProperty>
  
  <owl:Class rdf:ID="VintageJaar" />

  <owl:DatatypeProperty rdf:ID="jaarWaarde">
    <rdfs:domain rdf:resource="#VintageJaar" />    
    <rdfs:range  rdf:resource="&xsd;positiveInteger" />
  </owl:DatatypeProperty>
  
  <VintageJaar rdf:ID="Jaar1998">
    <jaarWaarde rdf:datatype="&xsd;positiveInteger">1998</jaarWaarde>
  </VintageJaar>

  <owl:ObjectProperty rdf:ID="het-vintage-jaar">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
    <rdfs:domain rdf:resource="#Vintage" />
    <rdfs:range  rdf:resource="#VintageJaar" />
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="is-gemaak-van-druiwe">
    <rdfs:subPropertyOf rdf:resource="&food;madeFromFruit" />
    <rdfs:domain rdf:resource="#Wyn" />
    <rdfs:range rdf:resource="#WynDruiwe" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="gemaak-in-wyn">
    <owl:inverseOf rdf:resource="#is-gemaak-van-druiwe" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="het-wyn-descriptor">
    <rdfs:domain rdf:resource="#Wyn" />
    <rdfs:range  rdf:resource="#WynDescriptor" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="het-suiker">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
    <rdfs:subPropertyOf rdf:resource="#het-wyn-descriptor" />
    <rdfs:range rdf:resource="#WynSuiker" />
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="het-Body">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
    <rdfs:subPropertyOf rdf:resource="#het-wyn-descriptor" />
    <rdfs:range rdf:resource="#WynBody" />
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="het-geur">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
    <rdfs:subPropertyOf rdf:resource="#het-wyn-descriptor" />
    <rdfs:range rdf:resource="#WynGeur" />
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="het-kleur">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
    <rdfs:subPropertyOf rdf:resource="#het-wyn-descriptor" />
    <rdfs:domain rdf:resource="#Wyn" />
    <rdfs:range rdf:resource="#WynKleur" />
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="het-maker">
    <rdf:type rdf:resource="&owl;FunctionalProperty" />
  </owl:ObjectProperty>
  
  <owl:ObjectProperty rdf:ID="produseer-wyn">
    <owl:inverseOf rdf:resource="#het-maker" />
  </owl:ObjectProperty>

  <owl:Class rdf:ID="Zinfandel">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Wyn" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:hasValue rdf:resource="#ZinfandelDruiwe" />
      </owl:Restriction>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:about="#Zinfandel">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-kleur" />
        <owl:hasValue rdf:resource="#Rooi" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-suiker" />
        <owl:hasValue rdf:resource="#Droe" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-Body" />
        <owl:allValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <owl:Thing rdf:about="#Vol" />
              <owl:Thing rdf:about="#Middelmatig" />
            </owl:oneOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-geur" />
        <owl:allValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <owl:Thing rdf:about="#Gematig" />
              <owl:Thing rdf:about="#Sterk" />
            </owl:oneOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:ID="wynmakery" />
  
  <owl:Class rdf:ID="WynDescriptor">
    <rdfs:comment>Made WynDescriptor unionType of Geurs and Kleur</rdfs:comment>
    <owl:unionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#WynGeur" />
      <owl:Class rdf:about="#WynKleur" />
    </owl:unionOf>
  </owl:Class>

  <owl:Class rdf:ID="WynGeur">
    <rdfs:subClassOf rdf:resource="#WynDescriptor" />
  </owl:Class>

  <owl:Class rdf:ID="WynKleur">
    <rdfs:subClassOf rdf:resource="#WynDescriptor" />
    <owl:oneOf rdf:parseType="Collection">
      <owl:Thing rdf:about="#Wit" />
      <owl:Thing rdf:about="#Roos" />
      <owl:Thing rdf:about="#Rooi" />
    </owl:oneOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WynSuiker">
    <rdfs:subClassOf rdf:resource="#WynGeur" />
    <owl:oneOf rdf:parseType="Collection">
      <owl:Thing rdf:about="#Soet" />
      <owl:Thing rdf:about="#AfDroe" />
      <owl:Thing rdf:about="#Droe" />
    </owl:oneOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WynGeur">
    <rdfs:subClassOf rdf:resource="#WynGeur" />
    <owl:oneOf rdf:parseType="Collection">
      <owl:Thing rdf:about="#Delikaat" />
      <owl:Thing rdf:about="#Gematig" />
      <owl:Thing rdf:about="#Sterk" />
    </owl:oneOf>
  </owl:Class>
  
  <owl:Class rdf:ID="WynBody">
    <rdfs:subClassOf rdf:resource="#WynGeur" />
    <owl:oneOf rdf:parseType="Collection">
      <owl:Thing rdf:about="#Lig" />
      <owl:Thing rdf:about="#Middelmatig" />
      <owl:Thing rdf:about="#Vol" />
    </owl:oneOf>
  </owl:Class>
  
  <Streek rdf:ID="USStreek" />
  
  <owl:Class rdf:ID="Toere">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Loire" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#gelee-in" />
        <owl:hasValue rdf:resource="#ToereStreek" />
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>

  <owl:Class rdf:about="#Toere">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:hasValue rdf:resource="#CheninBlancDruiwe" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:ID="Tafelwyn">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Wyn" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-suiker" />
        <owl:hasValue rdf:resource="#Droe" />
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="SoetWyn">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Wyn" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-suiker" />
        <owl:hasValue rdf:resource="#Soet" />
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="SoetRiesling">
    <rdfs:subClassOf rdf:resource="#NageregWyn" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-Body" />
        <owl:hasValue rdf:resource="#Vol" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-geur" />
        <owl:allValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <owl:Thing rdf:about="#Gematig" />
              <owl:Thing rdf:about="#Sterk" />
            </owl:oneOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Riesling" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-suiker" />
        <owl:hasValue rdf:resource="#Soet" />
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="StEmilion">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-kleur" />
        <owl:hasValue rdf:resource="#Rooi" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-geur" />
        <owl:hasValue rdf:resource="#Sterk" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:hasValue rdf:resource="#CabernetSauvignonDruiwe" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Bordeaux" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#gelee-in" />
        <owl:hasValue rdf:resource="#StEmilionStreek" />
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="SemillonOrSauvignonBlanc">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-kleur" />
        <owl:hasValue rdf:resource="#Wit" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-Body" />
        <owl:allValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <owl:Thing rdf:about="#Middelmatig" />
              <owl:Thing rdf:about="#Vol" />
            </owl:oneOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </rdfs:subClassOf>
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#Wyn" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:allValuesFrom>
          <owl:Class>
            <owl:oneOf rdf:parseType="Collection">
              <owl:Thing rdf:about="#SemillonDruiwe" />
              <owl:Thing rdf:about="#SauvignonBlancDruiwe" />
            </owl:oneOf>
          </owl:Class>
        </owl:allValuesFrom>
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="Semillon">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#SemillonOrSauvignonBlanc" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:hasValue rdf:resource="#SemillonDruiwe" />
      </owl:Restriction>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="SauvignonBlanc">
    <owl:intersectionOf rdf:parseType="Collection">
      <owl:Class rdf:about="#SemillonOrSauvignonBlanc" />
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:hasValue rdf:resource="#SauvignonBlancDruiwe" />
      </owl:Restriction>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#is-gemaak-van-druiwe" />
        <owl:maxCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:maxCardinality>
      </owl:Restriction>
    </owl:intersectionOf>
  </owl:Class>
  
  <owl:Class rdf:ID="Sauternes">
    <rdfs:subClassOf rdf:resource="#Laatoes" />
    <rdfs:subClassOf rdf:resource="#Bordeaux" />
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#gelee-in" />
        <owl:hasValue rdf:resource="#SauterneStreek" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-Body" />
        <owl:hasValue rdf:resource="#Middelmatig" />
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="#het-kleur" />
        <owl:hasValue rdf:resource="#Wit" />
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  
  <owl:Class rdf:ID="Sancerre">
